<Window x:Class="CreateDeckListJSONGUI.AskForFlavorQuoteWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CreateDeckListJSONGUI"
        mc:Ignorable="d"
        Title="AskForFlavorQuoteWindow" Height="200" Width="250">
    <StackPanel Margin="10">
        <TextBlock Text="Identifier:" />
        <TextBox x:Name="IdentifierBox" Margin="0,5,0,10"/>
        <TextBlock Text="Text:" />
        <TextBox x:Name="TextBox" Margin="0,5,0,10"/>
        <Button Content="OK" Click="OKButton_Click" />
    </StackPanel>
</Window>
